public class CenarioRegressivoGeral extends LoggedForTesting {

	@Test
	@Category({ Producao.class, SmokeTest.class })
	public void validarADMR04() {
		WDS.get().navigate().to(Ambiente.atual().programa("/ADM/ADMR04"));
		RegressivoMsgsPageObject.deve().esperarResultado();
		assertEquals("Relatório de Usuário", RegressivoMsgsPageObject.deve().pegarAlerta());
		// assertEquals("Usuário", RegressivoMsgsPageObject.deve().pegarLabelUsuario());
		assertEquals(" Ativo", RegressivoMsgsPageObject.deve().pegarLabelAtivo());
		assertEquals(" Inativo", RegressivoMsgsPageObject.deve().pegarLabelInativo());
		assertEquals(" Todos", RegressivoMsgsPageObject.deve().pegarLabelTodos());
	}

	@Test
	@Category({ Producao.class, SmokeTest.class })
	public void validarADMM07() {
		WDS.get().navigate().to(Ambiente.atual().programa("/ADM/ADMM07"));
		RegressivoMsgsPageObject.deve().esperarResultado();
		assertEquals("Dashboard", RegressivoMsgsPageObject.deve().pegarAlerta());
		assertEquals("Código", RegressivoMsgsPageObject.deve().pegarLabelPrimeiro());
		assertEquals("Nome", RegressivoMsgsPageObject.deve().pegarLabelSegundo());
		assertEquals("Descrição", RegressivoMsgsPageObject.deve().pegarLabelTerceiro());
		assertEquals("Status", RegressivoMsgsPageObject.deve().pegarLabelQuarto());
		RegressivoBotoesPageObject.deve().clicarAction();
		RegressivoMsgsPageObject.deve().esperarResultado();
		assertEquals(" Salvar", RegressivoMsgsPageObject.deve().pegarBtnSalvar());
	}

	@Test
	@Category({ Producao.class, SmokeTest.class })
	public void validarADMM14() {
		WDS.get().navigate().to(Ambiente.atual().programa("/ADM/ADMM14"));
		RegressivoMsgsPageObject.deve().esperarResultado();
		assertEquals("Manutenção de Parâmetros", RegressivoMsgsPageObject.deve().pegarAlerta());
		assertEquals("Parâmetro", RegressivoMsgsPageObject.deve().pegarLabelPrimeiro());
		assertEquals("Descrição", RegressivoMsgsPageObject.deve().pegarLabelSegundo());
		assertEquals("Abrangência", RegressivoMsgsPageObject.deve().pegarLabelTerceiro());
		assertEquals("Tipo de Dado", RegressivoMsgsPageObject.deve().pegarLabelQuarto());
		assertEquals("Valor Padrão", RegressivoMsgsPageObject.deve().pegarLabelQuinto());
		assertEquals("Data Inclusão", RegressivoMsgsPageObject.deve().pegarLabelSexto());
		assertEquals("Usuário Inclusão", RegressivoMsgsPageObject.deve().pegarLabelSetimo());
		assertEquals("Status", RegressivoMsgsPageObject.deve().pegarLabelOitavo());
		RegressivoBotoesPageObject.deve().clicarAction2();
		RegressivoMsgsPageObject.deve().esperarResultado();
		assertEquals("Salvar", RegressivoMsgsPageObject.deve().pegarBtnSalvar());
	}

	@Test
	@Category({ Producao.class, SmokeTest.class })
	public void validarADMM13() {
		WDS.get().navigate().to(Ambiente.atual().programa("/ADM/ADMM13"));
		RegressivoMsgsPageObject.deve().esperarResultado();
		assertEquals("Atualização Filial por Usuário", RegressivoMsgsPageObject.deve().pegarAlerta());
		assertEquals("Filial Origem e Destino", RegressivoMsgsPageObject.deve().pegarFilialOrigemDestino());
		assertEquals("Filial Espelho", RegressivoMsgsPageObject.deve().pegarFilialEspelho());
		assertEquals("Filial Destino", RegressivoMsgsPageObject.deve().pegarFilialDestino());
		assertEquals("Sem Espelho", RegressivoMsgsPageObject.deve().pegarLabelSemEspelho());
		assertEquals("Atualizar Acesso de Filial para Usuário", RegressivoMsgsPageObject.deve().pegarLabelAtualizar());
		assertEquals("Forma de Seleção", RegressivoMsgsPageObject.deve().pegarLabelSelecao());
		assertEquals("Dados de Origem:", RegressivoMsgsPageObject.deve().pegarLabelDados());
		assertEquals("Aplicar Acesso à Filial Destino:", RegressivoMsgsPageObject.deve().pegarLabelAplicar());
		RegressivoBotoesPageObject.deve().clicarAction3();
		RegressivoMsgsPageObject.deve().esperarResultado();
		assertEquals("Preenchimento obrigatório: Filial Destino", RegressivoMsgsPageObject.deve().pegarAlerta1());
		assertEquals("Selecione pelo menos um perfil ou um usuário.", RegressivoMsgsPageObject.deve().pegarAlerta2());
	}

